[global]
pid = /srv/deploy/<?= $this->id ?>.pid
error_log = /srv/deploy/logs/<?= $this->id ?>/error.log

[www]
listen = /srv/deploy/<?= $this->id ?>.sock
chroot = /srv/deploy/<?= $this->id ?>/srv/web
user = www-user
group = nobody
pm = dynamic
pm.max_children = 20
pm.start_servers = 10
pm.min_spare_servers = 5
pm.max_spare_servers = 20
